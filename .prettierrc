{
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderParserPlugins": [
    "typescript",
    "classProperties",
    "decorators-legacy"
  ],
  "importOrder": [
    "^@nestjs/(.*)$",
    "^@spaps/modules/(.*)$",
    "^@spaps/core/(.*)$",
    "^@spaps/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "singleQuote": true,
  "semi": false,
  "trailingComma": "all"
}
